interface interfaceName{
    (param1:string,param2:number):number;
}

interface interfaceNameGeneric<T>{
    (param1:T,param2:number):number;
}

#We can use above interfaces for functions

let funName:interfaceNameGeneric<string> = (param1:string,param2:number)=>{
    return 0;
};

