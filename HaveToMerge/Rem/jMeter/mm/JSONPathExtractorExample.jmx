        <hashTree>
          <com.atlantbh.jmeter.plugins.jsonutils.jsonpathextractor.JSONPathExtractor guiclass="com.atlantbh.jmeter.plugins.jsonutils.jsonpathextractor.gui.JSONPathExtractorGui" testclass="com.atlantbh.jmeter.plugins.jsonutils.jsonpathextractor.JSONPathExtractor" testname="jp@gc - JSON Path Extractor - Plain" enabled="true">
            <stringProp name="TestPlan.comments">This extractor sets variable &apos;Name&apos; value into value from JSON text. 
Run the test and look into View Results Tree.</stringProp>
            <stringProp name="VAR">ThumbData</stringProp>
            <stringProp name="JSONPATH">$.results[0].thumbnails[0].data</stringProp>
            <stringProp name="DEFAULT">FAILED</stringProp>
          </com.atlantbh.jmeter.plugins.jsonutils.jsonpathextractor.JSONPathExtractor>
          <hashTree/>
          <com.atlantbh.jmeter.plugins.jsonutils.jsonpathextractor.JSONPathExtractor guiclass="com.atlantbh.jmeter.plugins.jsonutils.jsonpathextractor.gui.JSONPathExtractorGui" testclass="com.atlantbh.jmeter.plugins.jsonutils.jsonpathextractor.JSONPathExtractor" testname="jp@gc - JSON Path Extractor - Array" enabled="true">
            <stringProp name="TestPlan.comments">This extractor sets variable &apos;Name&apos; value into value from JSON text. 
Run the test and look into View Results Tree.</stringProp>
            <stringProp name="VAR">ThumbID</stringProp>
            <stringProp name="JSONPATH">$.results[0].thumbnails[*]._id</stringProp>
            <stringProp name="DEFAULT">FAILED</stringProp>
          </com.atlantbh.jmeter.plugins.jsonutils.jsonpathextractor.JSONPathExtractor>
          <hashTree/>
          <com.atlantbh.jmeter.plugins.jsonutils.jsonpathassertion.JSONPathAssertion guiclass="com.atlantbh.jmeter.plugins.jsonutils.jsonpathassertion.gui.JSONPathAssertionGui" testclass="com.atlantbh.jmeter.plugins.jsonutils.jsonpathassertion.JSONPathAssertion" testname="jp@gc - JSON Path Assertion" enabled="true">
            <stringProp name="JSON_PATH">$.results[0].thumbnails[*].type</stringProp>
            <stringProp name="EXPECTED_VALUE">freeform</stringProp>
            <boolProp name="JSONVALIDATION">true</boolProp>
            <boolProp name="EXPECT_NULL">false</boolProp>
          </com.atlantbh.jmeter.plugins.jsonutils.jsonpathassertion.JSONPathAssertion>
          <hashTree/>
        </hashTree>
        <DebugSampler guiclass="TestBeanGUI" testclass="DebugSampler" testname="Debug Sampler" enabled="true">
